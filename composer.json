{
  "name": "perspective/magento2-partytown",
  "description": "Module for integrating BuilderIO/partytown library functionality into Magento 2 project",
  "type": "magento2-module",
  "version": "2.0.0",
  "license": [
    "OSL-3.0",
    "AFL-3.0"
  ],
  "require": {
    "php": "~8.1.0||~8.2.0",
    "magento/framework": "*",
    "npm-package": "^1.0.0"
  },
  "scripts": {
    "post-update-cmd": [
      "npm install",
      "npm run partytown"
    ],
    "post-install-cmd": [
      "npm install",
      "npm run partytown"
    ]
  },
  "autoload": {
    "files": [
      "registration.php"
    ],
    "psr-4": {
      "Perspective\\Partytown\\": ""
    }
  }
}